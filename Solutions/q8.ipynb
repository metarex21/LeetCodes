    "class Solution:\n",
    "    def mySqrt(self, x: int) -> int:\n",
    "        l=0;r=x\n",
    "        while r>=l:\n",
    "            mid = (l + r)//2\n",
    "\n",
    "            if mid*mid<=x:\n",
    "                l = mid+1\n",
    "            else:\n",
    "                r = mid-1       \n",
    "        return r      "
   ]
  }
