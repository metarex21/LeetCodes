    "class Solution:\n",
    "    def reverse(self, x: int) -> int:\n",
    "        temp=abs(x)\n",
    "        new=0\n",
    "        while temp>0:\n",
    "            new = new * 10 + temp%10\n",
    "            temp = temp//10\n",
    "        if (-2)**31<=new<2**31 and new!=0:\n",
    "            return new*x//abs(x)\n",
    "        else:return 0"
   ]
  }
